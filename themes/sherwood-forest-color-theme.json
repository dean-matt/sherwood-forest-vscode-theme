  "colors": {
    // Activity Bar - Bar that allows switching between side bar view, usually located on the far left
    "activityBar.activeBackground": "#ffffff20",
    "activityBar.background": "#0c3632",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#ffffff60",
    "activityBarBadge.background": "#00cc8b",
    "activityBarBadge.foreground": "#000000",

    // Badges - Small info labels, often little circles
    "badge.background": "#00cc8b",
    "badge.foreground": "#000000",

    // Breadcrumbs - Show for open files at the top of the editor
    "breadcrumb.activeSelectionForeground": "#00cc8b",
    "breadcrumb.background": "#030f0e",
    "breadcrumb.focusForeground": "#00cc8b",
    "breadcrumb.foreground": "#ffffff",

    // Buttons - A folder opened without
    "button.background": "#008f62",
    "button.foreground": "#ffffffd0",
    "button.hoverBackground": "#00cc8b",
    "button.secondaryBackground": "#ffffff40",
    "button.secondaryForeground": "#ffffffd0",
    "button.secondaryHoverBackground": "#ffffff60",

    // Checkboxes - Settings UI contains many of these
    "checkbox.background": "#000000",
    "checkbox.border": "#00000000",
    "checkbox.foreground": "#ffffffd0",

    // Debug Exception Widget - Shows when debugging stops at an exception
    "debugExceptionWidget.background": "#072423",
    "debugExceptionWidget.border": "#072423",

    // Debug Tool Bar - Shows when debugging an app, contains controls for reloading and stopping
    "debugToolBar.background": "#072423",
    "debugToolBar.border": "#072423",

    // Diff Editor - Shows when viewing changes to a file
    "diffEditor.border": "#82756460",
    "diffEditor.insertedTextBackground": "#00cc8b30",
    "diffEditor.insertedTextBorder": "#00000000",
    "diffEditor.removedTextBackground": "#e8643830",
    "diffEditor.removedTextBorder": "#00000000",

    // Dropdowns - Settings UI contains many of these
    "dropdown.background": "#003633",
    "dropdown.border": "#ffffff30",
    "dropdown.foreground": "#ffffffd0",

    // Editor General Settings
    "editor.background": "#030f0e",
    "editor.foldBackground": "#ffffff20",
    "editor.foreground": "#ffffffc0",
    "editor.hoverHighlightBackground": "#fe8e3560",
    "editor.inactiveSelectionBackground": "#82756460",

    // Editor Find Matches
    "editor.findMatchBackground": "#fe8e35a0",
    "editor.findMatchBorder": "#ffffffa0",
    "editor.findMatchHighlightBackground": "#00cc8b40",
    "editor.findMatchHighlightBorder": "#00000000",
    "editor.findMatchHighlightForeground": "#000000a0",
    "editor.findRangeHighlightBackground": "#ffffff15",
    "editor.findRangeHighlightBorder": "#00000000", // Causes lines to appear between ranges even when transparent

    // Editor Cursor
    "editorCursor.background": "#030f0e",
    "editorCursor.foreground": "#ffffffa0",

    // Editor Selection
    "editor.lineHighlightBackground": "#00000000",
    "editor.lineHighlightBorder": "#ffffff30",
    "editor.rangeHighlightBackground": "#fe8e3560",
    "editor.rangeHighlightBorder": "#00000000",
    "editor.selectionBackground": "#827564a0",
    "editor.selectionHighlightBackground": "#827564a0",
    "editor.selectionHighlightBorder": "#00000000",
    "editor.wordHighlightBackground": "#82756460",
    "editor.wordHighlightBorder": "#00000000",
    "editor.wordHighlightStrongBackground": "#827564a0",
    "editor.wordHighlightStrongBorder": "#00000000",
    "editorWhitespace.foreground": "#ffffff30",

    // Editor Squigglies
    // "editorError.background": "#e8643830", // Looks cleaner without this background
    "editorError.border": "#00000000",
    "editorError.foreground": "#e86438",
    // "editorInfo.background": "#00cc8b30", // Looks cleaner without this background
    "editorInfo.border": "#00000000",
    "editorInfo.foreground": "#00cc8b",
    // "editorWarning.background": "#fe8e3530", // Looks cleaner without this background
    "editorWarning.border": "#00000000",
    "editorWarning.foreground": "#ffba0a",

    // Editor Suggest Widget - aka Intellisense or Autocompletion
    "editorSuggestWidget.background": "#072423",
    "editorSuggestWidget.border": "#0c3632",
    "editorSuggestWidget.foreground": "#ffffffc0",
    "editorSuggestWidget.highlightForeground": "#00cc8b",
    "editorSuggestWidget.selectedBackground": "#ffffff60",

    // Editor Brackets
    "editorBracketMatch.background": "#ffffff30",
    "editorBracketMatch.border": "#ffffff60",

    // Editor Hover Widget - Such as hovering over a variable to see more info
    "editorHoverWidget.background": "#072423",
    "editorHoverWidget.border": "#0c3632",
    "editorHoverWidget.foreground": "#ffffffc0",

    // Editor Gutter - Located to the right of the line numbers
    "editorGutter.addedBackground": "#00cc8ba0",
    "editorGutter.background": "#030f0e",
    "editorGutter.commentRangeForeground": "#827564",
    "editorGutter.deletedBackground": "#e86438a0",
    "editorGutter.foldingControlForeground": "#ffffffc0",
    "editorGutter.modifiedBackground": "#ffba0a",

    // Editor Links - Such as holding Ctrl or Cmd and hovering over a class
    "editorLink.activeForeground": "#00cc8b",

    // Editor Marker Navigation - Such as hovering over an error and clicking "View Problem"
    "editorMarkerNavigation.background": "#072423",
    "editorMarkerNavigationError.background": "#e86438",
